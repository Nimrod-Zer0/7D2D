<config>
	<!-- Log with a warning message that mod is loaded (warning simply shows mod path cant be removed because that path doesnt exist) -->
	<remove xpath="/JankMod_Loaded[@modname='NRZ_7JankTweaks'][@filename='\Config\items.xml']" />

	<!-- Lower chance of food poisening for yucca fruit -->
	<set xpath="/items/item[@name='foodCropYuccaFruit']/effect_group/display_value[@name='FoodPoisoningRisk']/@value">0.03</set>	<!-- Default: 0.1 -->

	<!-- Lower chance of food poisening for potato -->
	<set xpath="/items/item[@name='foodCropPotato']/effect_group/display_value[@name='FoodPoisoningRisk']/@value">0.05</set>	<!-- Default: 0.3 -->

	<!-- Lower chance of food poisening for corn -->
	<set xpath="/items/item[@name='foodCropCorn']/effect_group/display_value[@name='FoodPoisoningRisk']/@value">0.05</set>	<!-- Default: 0.1 -->

	<!--	
	 	Credit goes to: Elucidus for this damage mod concept

		 	ToDo:
			 	fine tune damage bonuses - make sure top tier bow doesnt do more damage then top tier pistol for example.

			Notes:
				weapon mods like mag extender also give damage bonus even though there is no damage bonus listed in its config.
				i wondered if bonus was for addl accuracy but there is no bonus to shot spread or variance in config.
			 
	-->
	<!-- Remove Entity/Block Damage vars from all items -->
	<remove xpath="/items/item/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']"/>
	<remove xpath="/items/item/effect_group[@name='Base Effects']/passive_effect[@name='BlockDamage'][@operation='perc_add']"/>
	
	<!-- 
		Add Entity/Block Damage vars back to all items along with vars for Quality 
	=====================================================================
	This is done this way so all items even knives, axe ect get damage bonus based on its quality level.	
	-->
	<append xpath="/items/item/effect_group[@name='Base Effects']">
		<passive_effect name="EntityDamage" operation="perc_add" value="0,1" tier="1,6"/>
		<passive_effect name="BlockDamage" operation="perc_add" value="0,2" tier="1,6"/>
	</append>

	<!-- Modify Gun/Bow EntityDamage based on Quality Level -->
	<set xpath="/items/item[@name='gunAK47']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.35,1.35</set>	<!-- -.2,.8 -->
	<set xpath="/items/item[@name='gunSMG5']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.35,1.35</set>	<!-- -.2,.8 -->
	<set xpath="/items/item[@name='gunHuntingRifle']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.35,1.35</set>
	<set xpath="/items/item[@name='gunMR10']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.35,1.35</set>  <!-- .25,1.25 -->
	
	<set xpath="/items/item[@name='gunBlunderbuss']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.15,1.25</set>	<!-- minus .2,.8 -->
	<set xpath="/items/item[@name='gunPumpShotgun']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">-.2,.8</set>	<!-- -.2,.8 -->
	
	<set xpath="/items/item[@name='gunCompoundBow']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.2,1.2</set>	<!-- .3,1.3 -->
	<set xpath="/items/item[@name='gunCrossbow']/effect_group[@name='Base Effects']/passive_effect[@name='EntityDamage'][@operation='perc_add']/@value">.1,1.0</set>	<!-- .45,1.45 -->

	<!-- Compound bow now shoots closer to the effective range of a crossbow like it should -->
	<set xpath="/items/item[@name='gunCompoundBow']/effect_group[@name='Base Effects']/passive_effect[@name='ProjectileVelocity'][@operation='perc_add']/@value">3.5</set>	<!-- 0.2 -->
	<set xpath="/items/item[@name='gunCompoundBow']/effect_group[@name='Base Effects']/passive_effect[@name='DamageFalloffRange'][@operation='base_set']/@value">30</set>	<!-- 30 -->

	<!-- Increase MR10 Sniper Rifle Ammo Capacity -->
	<set xpath="/items/item[@name='gunMR10']/effect_group[@name='Base Effects']/passive_effect[@name='MagazineSize']/@value">10</set>

